generator client {
  provider = "prisma-client-js" //The file that will be created with .js
  //What will be used to import on that file
}

datasource db {
  provider = "postgresql" //Which SQL to generate language to use
 
}

model User {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    @unique
  password  String
  createdAT DateTime  @default(now())
}

model Expense {
  id         Int       @id @default(autoincrement())
  name       String 
  amount     Float 
  category   String
  createdAt  DateTime  @default(now())
}

